[32m+[m[32mdocument.addEventListener('DOMContentLoaded', function() {[m
[32m+[m[32m    // ========== CONTROLE DO FORMUL√ÅRIO ==========[m
[32m+[m[32m    const petvipForm = document.getElementById('petvipForm');[m
[32m+[m[41m    [m
[32m+[m[32m    if (petvipForm) {[m
[32m+[m[32m        // Elementos do formul√°rio[m
[32m+[m[32m        const nomeInput = document.getElementById('nome');[m
[32m+[m[32m        const emailInput = document.getElementById('email');[m
[32m+[m[32m        const telefoneInput = document.getElementById('telefone');[m
[32m+[m[32m        const feedbackDiv = document.createElement('div');[m
[32m+[m[41m        [m
[32m+[m[32m        // Configura elementos din√¢micos[m
[32m+[m[32m        feedbackDiv.className = 'form-feedback';[m
[32m+[m[32m        feedbackDiv.setAttribute('role', 'alert');[m
[32m+[m[32m        petvipForm.appendChild(feedbackDiv);[m
[32m+[m[41m        [m
[32m+[m[32m        // M√°scara de telefone[m
[32m+[m[32m        if (telefoneInput) {[m
[32m+[m[32m            telefoneInput.addEventListener('input', function(e) {[m
[32m+[m[32m                let value = e.target.value.replace(/\D/g, '');[m
[32m+[m[32m                if (value.length > 11) value = value.substring(0, 11);[m
[32m+[m[41m                [m
[32m+[m[32m                // Formata√ß√£o: (00) 00000-0000[m
[32m+[m[32m                if (value.length > 2) {[m
[32m+[m[32m                    value = `(${value.substring(0, 2)}) ${value.substring(2)}`;[m
[32m+[m[32m                }[m
[32m+[m[32m                if (value.length > 10) {[m
[32m+[m[32m                    value = `${value.substring(0, 10)}-${value.substring(10)}`;[m
[32m+[m[32m                }[m
[32m+[m[41m                [m
[32m+[m[32m                e.target.value = value;[m
[32m+[m[32m            });[m
[32m+[m[41m            [m
[32m+[m[32m            // Adiciona valida√ß√£o de telefone[m
[32m+[m[32m            telefoneInput.addEventListener('blur', validateTelefone);[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        // Valida√ß√£o em tempo real[m
[32m+[m[32m        if (nomeInput) nomeInput.addEventListener('blur', validateNome);[m
[32m+[m[32m        if (emailInput) emailInput.addEventListener('blur', validateEmail);[m
[32m+[m[41m        [m
[32m+[m[32m        // Fun√ß√µes de valida√ß√£o[m
[32m+[m[32m        function validateNome() {[m
[32m+[m[32m            const nomeGroup = nomeInput.closest('.form-group');[m
[32m+[m[32m            if (!nomeGroup) return false;[m
[32m+[m[41m        [m
[32m+[m[32m            let errorMsg = nomeGroup.querySelector('.error-message');[m
[32m+[m[32m            if (!errorMsg) {[m
[32m+[m[32m                errorMsg = document.createElement('small');[m
[32m+[m[32m                errorMsg.className = 'error-message';[m
[32m+[m[32m                nomeGroup.appendChild(errorMsg);[m
[32m+[m[32m            }[m
[32m+[m[41m        [m
[32m+[m[32m            if (nomeInput.value.trim().length < 3) {[m
[32m+[m[32m                nomeGroup.classList.add('error');[m
[32m+[m[32m                errorMsg.textContent = 'Por favor, insira seu nome completo';[m
[32m+[m[32m                return false;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                nomeGroup.classList.remove('error');[m
[32m+[m[32m                errorMsg.textContent = '';[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        function validateEmail() {[m
[32m+[m[32m            const emailGroup = emailInput.closest('.form-group');[m
[32m+[m[32m            if (!emailGroup) return false;[m
[32m+[m[41m            [m
[32m+[m[32m            const re = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;[m
[32m+[m[41m            [m
[32m+[m[32m            if (!re.test(emailInput.value.trim())) {[m
[32m+[m[32m                emailGroup.classList.add('error');[m
[32m+[m[32m                const errorMsg = emailGroup.querySelector('.error-message');[m
[32m+[m[32m                if (errorMsg) errorMsg.textContent = 'Por favor, insira um e-mail v√°lido';[m
[32m+[m[32m                return false;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                emailGroup.classList.remove('error');[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        function validateTelefone() {[m
[32m+[m[32m            const telefoneGroup = telefoneInput.closest('.form-group');[m
[32m+[m[32m            if (!telefoneGroup) return false;[m
[32m+[m[41m            [m
[32m+[m[32m            const value = telefoneInput.value.replace(/\D/g, '');[m
[32m+[m[32m            if (value.length < 10 || value.length > 11) {[m
[32m+[m[32m                telefoneGroup.classList.add('error');[m
[32m+[m[32m                const errorMsg = telefoneGroup.querySelector('.error-message');[m
[32m+[m[32m                if (errorMsg) errorMsg.textContent = 'Por favor, insira um telefone v√°lido';[m
[32m+[m[32m                return false;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                telefoneGroup.classList.remove('error');[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        function showFeedback(message, type) {[m
[32m+[m[32m            feedbackDiv.textContent = message;[m
[32m+[m[32m            feedbackDiv.className = 'form-feedback';[m
[32m+[m[32m            feedbackDiv.classList.add(type);[m
[32m+[m[32m            feedbackDiv.setAttribute('aria-live', 'polite');[m
[32m+[m[41m            [m
[32m+[m[32m            setTimeout(() => {[m
[32m+[m[32m                feedbackDiv.classList.remove(type);[m
[32m+[m[32m            }, 5000);[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        // Adiciona elementos de erro dinamicamente[m
[32m+[m[32m        document.querySelectorAll('.form-group').forEach(group => {[m
[32m+[m[32m            if (!group.querySelector('.error-message')) {[m
[32m+[m[32m                const errorElement = document.createElement('small');[m
[32m+[m[32m                errorElement.className = 'error-message';[m
[32m+[m[32m                group.appendChild(errorElement);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        // Configura o bot√£o do Mercado Pago[m
[32m+[m[32m        const mpButton = document.querySelector('[name="MP-payButton"]');[m
[32m+[m[32m        if (mpButton) {[m
[32m+[m[32m            mpButton.addEventListener('click', async function(e) {[m
[32m+[m[32m                e.preventDefault();[m
[32m+[m[41m            [m
[32m+[m[32m                if (!validateNome() || !validateEmail()) {[m
[32m+[m[32m                    showFeedback('Por favor, preencha nome e e-mail corretamente.', 'error');[m
[32m+[m[32m                    return;[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                try {[m
[32m+[m[32m                    // Armazena os dados temporariamente[m
[32m+[m[32m                    const petTypeRadio = document.querySelector('input[name="pet_type"]:checked');[m
[32m+[m[32m                    if (!petTypeRadio) {[m
[32m+[m[32m                        showFeedback('Por favor, selecione o tipo do seu pet.', 'error');[m
[32m+[m[32m                        return;[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    const leadData = {[m
[32m+[m[32m                        nome: nomeInput.value.trim(),[m
[32m+[m[32m                        email: emailInput.value.trim(),[m
[32m+[m[32m                        telefone: telefoneInput ? telefoneInput.value.replace(/\D/g, '') : '',[m
[32m+[m[32m                        petType: petTypeRadio.value[m
[32m+[m[32m                    };[m
[32m+[m
[32m+[m[32m                    // Mostra feedback de carregamento[m
[32m+[m[32m                    showFeedback('Processando sua assinatura...', 'info');[m
[32m+[m[41m                    [m
[32m+[m[32m                    // Envia para o backend[m
[32m+[m[32m                    const response = await fetch('/create-subscription', {[m
[32m+[m[32m                        method: 'POST',[m
[32m+[m[32m                        headers: {[m
[32m+[m[32m                            'Content-Type': 'application/json',[m
[32m+[m[32m                            'Accept': 'application/json'[m
[32m+[m[32m                        },[m
[32m+[m[32m                        body: JSON.stringify(leadData)[m
[32m+[m[32m                    });[m
[32m+[m
[32m+[m[32m                    // Verifica√ß√£o completa da resposta[m
[32m+[m[32m                    if (!response) {[m
[32m+[m[32m                        throw new Error('Sem resposta do servidor');[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    if (!response.ok) {[m
[32m+[m[32m                        const errorText = await response.text();[m
[32m+[m[32m                        throw new Error(errorText || 'Erro no servidor');[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    const data = await response.json();[m
[32m+[m[41m                    [m
[32m+[m[32m                    if (!data.payment_url && !data.sandbox_init_point && !data.init_point) {[m
[32m+[m[32m                        throw new Error('URL de pagamento n√£o recebida');[m
[32m+[m[32m                    }[m
[32m+[m
[32m+[m[32m                    // Redireciona para o checkout do Mercado Pago[m
[32m+[m[32m                    const paymentUrl = data.payment_url ||[m[41m [m
[32m+[m[32m                                      (process.env.NODE_ENV === 'production' ? data.init_point : data.sandbox_init_point);[m
[32m+[m[41m                    [m
[32m+[m[32m                    window.location.href = paymentUrl;[m
[32m+[m[41m                    [m
[32m+[m[32m                } catch (error) {[m
[32m+[m[32m                    console.error('Erro no processo de assinatura:', error);[m
[32m+[m[32m                    showFeedback([m
[32m+[m[32m                        error.message.includes('Failed to fetch')[m[41m [m
[32m+[m[32m                            ? 'Erro de conex√£o com o servidor'[m[41m [m
[32m+[m[32m                            : error.message,[m
[32m+[m[32m                        'error'[m
[32m+[m[32m                    );[m
[32m+[m[32m                }[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // ========== FAQ ACCORDION ==========[m
[32m+[m[32m    const faqItems = document.querySelectorAll('.faq__item');[m
[32m+[m[41m    [m
[32m+[m[32m    if (faqItems.length > 0) {[m
[32m+[m[32m        faqItems.forEach(item => {[m
[32m+[m[32m            const question = item.querySelector('.faq__question');[m
[32m+[m[32m            const answer = item.querySelector('.faq__answer');[m
[32m+[m[41m        [m
[32m+[m[32m            if (question && answer) {[m
[32m+[m[32m                question.setAttribute('role', 'button');[m
[32m+[m[32m                question.setAttribute('tabindex', '0');[m
[32m+[m[41m        [m
[32m+[m[32m                const toggleFAQ = () => {[m
[32m+[m[32m                    faqItems.forEach(otherItem => {[m
[32m+[m[32m                        if (otherItem !== item) {[m
[32m+[m[32m                            otherItem.classList.remove('active');[m
[32m+[m[32m                            const otherAnswer = otherItem.querySelector('.faq__answer');[m
[32m+[m[32m                            if (otherAnswer) otherAnswer.style.maxHeight = '0';[m
[32m+[m[32m                        }[m
[32m+[m[32m                    });[m
[32m+[m[41m        [m
[32m+[m[32m                    const isActive = item.classList.toggle('active');[m
[32m+[m[41m        [m
[32m+[m[32m                    if (isActive) {[m
[32m+[m[32m                        answer.style.maxHeight = answer.scrollHeight + 'px';[m
[32m+[m[32m                        answer.setAttribute('aria-hidden', 'false');[m
[32m+[m[32m                    } else {[m
[32m+[m[32m                        answer.style.maxHeight = '0';[m
[32m+[m[32m                        answer.setAttribute('aria-hidden', 'true');[m
[32m+[m[32m                    }[m
[32m+[m[41m        [m
[32m+[m[32m                    const icon = question.querySelector('i');[m
[32m+[m[32m                    if (icon) {[m
[32m+[m[32m                        icon.style.transform = isActive ? 'rotate(180deg)' : 'rotate(0)';[m
[32m+[m[32m                    }[m
[32m+[m[32m                };[m
[32m+[m[41m        [m
[32m+[m[32m                const handleFAQInteraction = (e) => {[m
[32m+[m[32m                    if (e.type === 'keydown' && e.key !== 'Enter' && e.key !== ' ') return;[m
[32m+[m[32m                    if (e.type === 'keydown') e.preventDefault();[m
[32m+[m[32m                    toggleFAQ();[m
[32m+[m[32m                };[m
[32m+[m[41m        [m
[32m+[m[32m                question.addEventListener('click', handleFAQInteraction);[m
[32m+[m[32m                question.addEventListener('keydown', handleFAQInteraction);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // ========== SCROLL SUAVE ==========[m
[32m+[m[32m    document.querySelectorAll('a[href^="#"]').forEach(anchor => {[m
[32m+[m[32m        anchor.addEventListener('click', function(e) {[m
[32m+[m[32m            e.preventDefault();[m
[32m+[m[32m            const targetId = this.getAttribute('href');[m
[32m+[m[32m            if (targetId === '#') return;[m
[32m+[m[41m            [m
[32m+[m[32m            const targetElement = document.querySelector(targetId);[m
[32m+[m[41m            [m
[32m+[m[32m            if (targetElement) {[m
[32m+[m[32m                targetElement.scrollIntoView({[m
[32m+[m[32m                    behavior: 'smooth',[m
[32m+[m[32m                    block: 'start'[m
[32m+[m[32m                });[m
[32m+[m[41m                [m
[32m+[m[32m                // Melhoria de acessibilidade: foco no elemento alvo[m
[32m+[m[32m                setTimeout(() => {[m
[32m+[m[32m                    targetElement.setAttribute('tabindex', '-1');[m
[32m+[m[32m                    targetElement.focus();[m
[32m+[m[32m                }, 1000);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    });[m
[32m+[m[32m});[m
\ No newline at end of file[m
